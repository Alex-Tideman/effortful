<div class="row">
<% @efforts.each do |effort| %>
      <div class="col s12 m2">
        <div class="card darken-1">
          <div class="card-content">
            <span class="card-title"><%= link_to effort.title, user_effort_path(user: effort.member, id: effort.id) %></span>
            <p><%= effort.member.name %></p>
          </div>
        </div>
      </div>
<% end %>
</div>